{
  "name": "mvp-app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "build:deploy": "npx prisma migrate deploy && npx prisma generate && next build",
    "start": "next start",
    "lint": "eslint",
    "seed": "node prisma/seed.mjs",
    "test": "npm run test:smoke",
    "test:smoke": "node scripts/smoke-test.mjs",
    "preflight:deploy": "bash scripts/preflight-deploy.sh",
    "prisma:validate:sqlite": "PRISMA_SCHEMA_PATH=prisma/schema.prisma npx prisma validate",
    "prisma:validate:postgres": "PRISMA_SCHEMA_PATH=prisma/schema.postgres.prisma DATABASE_URL=postgresql://postgres:postgres@localhost:5432/tabolla?schema=public npx prisma validate",
    "prisma:migrate:deploy:postgres": "PRISMA_SCHEMA_PATH=prisma/schema.postgres.prisma npx prisma migrate deploy"
  },
  "dependencies": {
    "@dnd-kit/core": "^6.3.1",
    "@dnd-kit/utilities": "^3.2.2",
    "@prisma/client": "^6.16.0",
    "next": "16.1.6",
    "pdf-parse": "^2.4.5",
    "prisma": "^6.16.0",
    "react": "19.2.3",
    "react-dom": "19.2.3"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "eslint": "^9",
    "eslint-config-next": "16.1.6",
    "tailwindcss": "^4",
    "typescript": "^5"
  }
}
